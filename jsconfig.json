{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@layouts": ["src/layouts"],
      "@layouts/*": ["src/layouts/*"],
    },
    "checkJs": true,
    "allowJs": true,
  }
}
